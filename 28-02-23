#include <stdio.h>

int main() {
    int n;
    printf("Enter the value of n: ");
    scanf("%d", &n);
    for (int i = 1; i <= n; i++) {
        // Print spaces before the stars
        for (int j = 1; j <= n - i; j++) {
            printf("* ");
        }
        
        for (int k = 1; k <= i; k++) {
            printf("* ");
        }
        printf("\n"); 
    }
    return 0;
}
